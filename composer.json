{
  "name": "civicrm/vitid_auth0",
  "description": "VIT ID (Auth0) Authentication for CiviCRM Standalone",
  "type": "civicrm-ext",
  "license": "AGPL-3.0",
  "require": {
    "php": "^8.2",
    "auth0/auth0-php": "^8.17",
    "nyholm/psr7": "^1.8",
    "psr/log": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "CRM\\VitidAuth0\\": "CRM/VitidAuth0/"
    }
  },
  "extra": {
    "civicrm": {
      "ext-name": "vitid_auth0"
    }
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
